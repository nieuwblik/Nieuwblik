{
  "redirects": [
    {
      "source": "/services",
      "destination": "/diensten",
      "permanent": true
    },
    {
      "source": "/about",
      "destination": "/over-ons",
      "permanent": true
    },
    {
      "source": "/start-your-project",
      "destination": "/start-je-project",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
